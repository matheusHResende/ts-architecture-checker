{
    "analyzer": {
        "files": [
            "./src/analyzer/*"
        ],
        "allowed": [
            "util"
        ]
    },
    "cli": {
        "files": [
            "./src/cli/**"
        ],
        "allowed": [
            "tsarch",
            " util"
        ]
    },
    "conformance": {
        "files": [
            "./src/conformancer/*"
        ],
        "allowed": [
            "analyzer",
            "ruler"
        ],
        "required": [
            "ruler"
        ]
    },
    "visualizer": {
        "files": [
            "./src/visualizer/*"
        ],
        "allowed": [
            "util"
        ]
    },
    "tsarch": {
        "files": [
            "./src/tsarch/*"
        ],
        "forbidden": [
            "cli"
        ]
    },
    "util": {
        "files": [
            "./src/utils/*"
        ]
    },
    "ruler": {
        "files": [
            "./src/ruler/*"
        ]
    }
}